                          include include/agelist
                    .row
                      .col-xs-12
                        label(for="") Preis
                      .col-xs-12.padding-bottom-12
                        select.form-control
                          include include/pricelist
                      .col-xs-6.padding-right-6
                        .input-group
                          span.input-group-addon &#8364;
                          input.form-control.price-input(type="email", placeholder="min")
