                    .row
                      .col-xs-12
                        label(for="") Marke
                      .col-xs-12.padding-bottom-12
                        select.form-control
                          include include/brandlist
                    .row
                      .col-xs-12
                        label(for="") Kategorie
                      .col-xs-12.padding-bottom-12
                        select.form-control
                          include include/categorylist
                    .row
                      .col-xs-12
                        label(for="") Geschlecht
                      .col-xs-12.padding-bottom-12
                        select.form-control
                          include include/genderlist
                    .row
                      .col-xs-12
                        label(for="") Alter
                      .col-xs-12.padding-bottom-12
